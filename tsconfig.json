{
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "crypto": [
          "node_modules/crypto-js"
        ],
        "~/*": [
          "./src/*"
        ],
      },
      "allowSyntheticDefaultImports": true,
      "jsx": "react-native",
      "lib": [
        "dom",
        "esnext"
      ],
      "moduleResolution": "node",
      "noEmit": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "strict": true
    },
    "extends": "expo/tsconfig.base"
  }
  